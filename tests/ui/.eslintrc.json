{
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "require": false,
    "expect": false,
    "browser": false,
    "testHelpers": false,
    "$": false,
    "$$": false
  },
  "rules": {
    "class-methods-use-this": [0],
    "no-underscore-dangle": [0],
    "no-unused-expressions": [0]
  }
}
