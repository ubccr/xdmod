                    "name": { "type": "string" },
                    "fields": { "$ref": "#/$defs/fields" }
                },
                "required": [ "id", "name", "fields" ],
                "additionalProperties": false
            },
            "jobs_realm": {
                "properties": {
                    "data": {
                        "contains": {
                            "properties": {
                                "id": { "enum": [ "Jobs" ] },
                                "name": { "enum": [ "Jobs" ] },
