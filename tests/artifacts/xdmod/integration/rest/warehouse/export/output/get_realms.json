                                    "minItems": 29,
                                    "maxItems": 29
                                }
                            }
                        },
                        "minContains": 1,
                        "maxContains": 1
                    }
                }
            },
            "cloud_realm": {
                "properties": {
                    "data": {
                        "contains": {
                            "properties": {
                                "id": { "enum": [ "Cloud" ] },
                                "name": { "enum": [ "Cloud" ] },
                                "fields": {
                                    "minItems": 16,
                                    "maxItems": 16
                                }
                            }
                        },
                        "minContains": 1,
                        "maxContains": 1
                    }
                }
            },
            "fields": {
                "type": "array",
                "items": { "$ref": "#/$defs/field" },
