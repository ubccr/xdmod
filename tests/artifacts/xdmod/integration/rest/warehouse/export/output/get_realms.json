            },
            "field": {
                "type": "object",
                "properties": {
                    "name": { "type": "string" },
                    "alias": { "type": "string" },
                    "display": { "type": "string" },
                    "anonymize": { "type": "boolean" },
                    "documentation": { "type": "string" }
                },
