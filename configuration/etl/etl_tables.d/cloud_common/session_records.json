{
    "table_definition": {
        "name": "session_records",
        "comment": "The start and end times for cloud instances reconstructed from event data.",
        "engine": "InnoDB",
        "columns": [
            {
                "name": "session_id",
                "type": "bigint(20) unsigned",
                "extra": "auto_increment",
                "nullable": false
            },
            {
                "name": "instance_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "resource_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "start_time",
                "type": "datetime",
                "nullable": false
            },
            {
                "name": "start_event_type_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "end_time",
                "type": "datetime",
                "nullable": false
            },
            {
                "name": "end_event_type_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "instance_type",
                "type": "varchar(64)",
                "nullable": false
            },
            {
                "name": "instance_type_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "num_cores",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "memory_mb",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "processorbucket_id",
                "type": "int(4)",
                "nullable": false,
                "comment": "DIMENSION: Pre-determined processor bucket sizes. References processor_buckets.id"
            },
            {
                "name": "memorybucket_id",
                "type": "int(4)",
                "nullable": false,
                "comment": "DIMENSION: Pre-determined memory bucket sizes. References memory_buckets.id"
            },
            {
                "name": "disk_gb",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "start_time_ts",
                "type": "bigint(18)",
                "nullable": false
            },
            {
                "name": "end_time_ts",
                "type": "bigint(18)",
                "nullable": false
            },
            {
                "name": "start_day_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "end_day_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "wallduration",
                "type": "bigint(18)",
                "nullable": true
            },
            {
                "name": "person_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "systemaccount_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "submission_venue_id",
                "type": "int(5)",
                "nullable": true
            },
            {
                "name": "domain_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "last_modified",
                "type": "timestamp",
                "nullable": false,
                "default": "CURRENT_TIMESTAMP",
                "extra": "on update CURRENT_TIMESTAMP"
            },
            {
                "name": "service_provider",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "account_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "principalinvestigator_person_id",
                "type": "int(11)",
                "nullable": false,
                "default": -1,
                "comment": "DIMENSION: The PI that owns the allocations that these VM's ran under. References principalinvestigator.person_id"
            },
            {
                "name": "fos_id",
                "type": "int(11)",
                "nullable": false,
                "default": 1,
                "comment": "DIMENSION: The field of science of the project to which the jobs belong"
            },
            {
                "name": "host_id",
                "type": "int(11)",
                "nullable": false,
                "default": 1,
                "comment": "DIMENSION: The host the VM is located on"
            },
            {
                "name": "instance_state_id",
                "type": "int(11)",
                "nullable": true,
                "default": null,
                "comment": "DIMENSION: The state of the VM"
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "resource_id",
                    "instance_id",
                    "start_time_ts",
                    "start_event_type_id"
                ],
                "is_unique": true
            },
            {
                "name": "increment_key",
                "columns": ["session_id"],
                "is_unique": true
            },
            {
                "name": "index_last_modified",
                "columns": [
                    "last_modified"
                ]
            },
            {
                "name": "session_instance_id",
                "columns": [
                    "instance_id"
                ],
                "is_unique": false
            }
        ]
    }
}
