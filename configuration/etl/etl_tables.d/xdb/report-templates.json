{
    "table_definition": {
        "name": "ReportTemplates",
        "engine": "MyISAM",
        "columns": [
            {
                "name": "id",
                "type": "int(11)",
                "nullable": false,
                "extra": "auto_increment"
            },
            {
                "name": "name",
                "type": "varchar(1000)"
            },
            {
                "name": "description",
                "type": "varchar(1000)"
            },
            {
                "name": "template",
                "type": "varchar(30)"
            },
            {
                "name": "title",
                "type": "varchar(1000)"
            },
            {
                "name": "header",
                "type": "varchar(1000)"
            },
            {
                "name": "footer",
                "type": "varchar(1000)"
            },
            {
                "name": "format",
                "type": "enum('Pdf','Pptx','Doc','Xls','Html')"
            },
            {
                "name": "font",
                "type": "enum('Times','Arial')"
            },
            {
                "name": "schedule",
                "type": "enum('Once','Daily','Weekly','Monthly','Quarterly','Semi-annually','Annually')"
            },
            {
                "name": "delivery",
                "type": "enum('Download','E-mail')"
            },
            {
                "name": "charts_per_page",
                "type": "int(1)"
            },
            {
                "name": "use_submenu",
                "type": "tinyint(1)"
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "id"
                ],
                "type": "BTREE",
                "is_unique": true
            }
        ],
        "triggers": []
    }
}
