{
    "table_definition": {
        "name": "instance_type",
        "engine": "MyISAM",
        "comment": "Instance type configurations. Values for the same name may change over time.",
        "columns": [
            {
                "name": "instance_type_id",
                "type": "int(11)",
                "nullable": false,
                "extra": "auto_increment"
            },
            {
                "name": "instance_type",
                "type": "varchar(64)",
                "nullable": false,
                "comment": "Short version or abbrev"
            },
            {
                "name": "display",
                "type": "varchar(256)",
                "nullable": false,
                "comment": "What to show the user"
            },
            {
                "name": "description",
                "type": "varchar(1024)",
                "nullable": true,
                "default": null
            },
            {
                "name": "num_cores",
                "type": "int(11)",
                "nullable": false,
                "default": 0
            },
            {
                "name": "memory_mb",
                "type": "int(11)",
                "nullable": false,
                "default": 0
            },
            {
                "name": "disk_gb",
                "type": "int(11)",
                "nullable": false,
                "default": 0,
                "comment": "Disk size configured in image"
            },
            {
                "name": "start_time",
                "type": "timestamp",
                "nullable": false,
                "default": "0000-00-00 00:00:00",
                "comment": "End time for this configuration"
            },
            {
                "name": "end_time",
                "type": "timestamp",
                "nullable": true,
                "default": null,
                "comment": "End time for this configuration"
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "instance_type_id"
                ],
                "type": "BTREE",
                "is_unique": true
            }
        ],
        "triggers": []
    },

    "#": "Define the mapping between the source value fields and the table columns.",
    "destination_columns": ["instance_type_id", "instance_type", "display", "description"],
    
    "#": "Provide the raw source values.",
    "source_values": [
        [-1, "unknown",            "Unknown",               "Unknown instance type" ]
    ]
}
