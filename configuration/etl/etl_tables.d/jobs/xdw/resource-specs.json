{
    "table_definition": {
        "name": "resourcespecs",
        "engine": "InnoDB",
        "columns": [
            {
                "name": "resource_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "start_date_ts",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "end_date_ts",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "start_day_id",
                "type": "int(11)",
                "nullable": false
            },
            {
                "name": "end_day_id",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "cpu_processor_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "cpu_node_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "cpu_processor_count_per_node",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "gpu_processor_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "gpu_node_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "gpu_processor_count_per_node",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "comments",
                "type": "varchar(500)",
                "nullable": true
            },
            {
                "name": "su_available_per_day",
                "type": "int(10) unsigned",
                "nullable": true,
                "comment": "Total number of SU's available per day"
            },
            {
                "name": "name",
                "type": "varchar(200)",
                "nullable": true
            },
            {
                "name": "last_modified",
                "type": "timestamp",
                "nullable": false,
                "default": "CURRENT_TIMESTAMP",
                "extra": "on update CURRENT_TIMESTAMP"
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "resource_id",
                    "start_date_ts"
                ],
                "type": "BTREE",
                "is_unique": true
            },
            {
                "name": "unq",
                "columns": [
                    "name",
                    "start_date_ts"
                ],
                "type": "BTREE",
                "is_unique": false
            }
        ],
        "triggers": []
    }
}
