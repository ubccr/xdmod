{
    "table_definition": {
        "$ref": "etl_tables.d/xdmod/queue.json#/table_definition"
    },
    "source_query": {
        "records": {
            "id": "DISTINCT COALESCE(queue, 'NA')",
            "resource_id": "resource_id"
        },
        "joins": [
            {
                "name": "hpcdb_jobs",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "j"
            }
        ],
        "where": [
            "j.nodecount <> 0",
            "j.nodecount IS NOT NULL"
        ]
    }
}
