{
    "defaults": {
        "global": {
            "endpoints": {
                "source": {
                    "type": "mysql",
                    "name": "blade source",
                    "config": "datawarehouse",
                    "schema": "${blade}-modw"
                },
                "destination": {
                    "type": "mysql",
                    "name": "Cloud timing test DB",
                    "config": "datawarehouse",
                    "schema": "modw"
                }
            }
        },
        "fed.${blade}.bootstrap": {
            "endpoints": {
                "destination": {
                    "type": "mysql",
                    "name": "blade source",
                    "config": "datawarehouse",
                    "schema": "${blade}-modw",
                    "create_schema_if_not_exists": true
                }
            },
            "class": "StructuredFileIngestor",
            "namespace": "ETL\\\\\\Ingestor",
            "options_class": "IngestorOptions",
            "enabled": true
        },
        "fed.${blade}.ingest": {
            "class": "DatabaseIngestor",
            "namespace": "ETL\\\\\\Ingestor",
            "options_class": "IngestorOptions",
            "enabled": true
        }
    },
    "fed.${blade}.bootstrap": {
        "$ref": "etl_pipelines.d/datawarehouse.json#/bootstrap"
    },
    "fed.${blade}.ingest": {
        "$ref": "etl_pipelines.d/federation.json#/ingest"
    }
}
