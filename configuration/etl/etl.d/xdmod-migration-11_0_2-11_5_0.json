            "class": "StructuredFileIngestor",
            "endpoints": {
                "source": {
                    "type": "jsonfile",
                    "name": "Resources configuration",
                    "path": "${base_dir}/../resources.json",
                    "record_schema_path": "common/resources.schema.json",
                    "field_names": [
                        "resource",
                        "name",
                        "description",
                        "resource_type",
                        "pi_column",
                        "shared_jobs",
                        "timezone",
                        "resource_allocation_type",
                        "organization"
                    ]
                },
                "destination": {
                    "type": "mysql",
                    "name": "Shredder/Staging Database",
                    "config": "database",
                    "schema": "mod_shredder"
                }
            }
        },
