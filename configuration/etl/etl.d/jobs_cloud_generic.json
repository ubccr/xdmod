      {
          "name": "GenericCloudCloudStagingPersonIngestor",
          "description": "Cloud account data",
          "class": "DatabaseIngestor",
          "definition_file": "jobs/xdw/person.json",
            "endpoints": {
                "source": {
                    "type" : "mysql",
                    "name" : "Cloud DB",
                    "config" : "datawarehouse",
