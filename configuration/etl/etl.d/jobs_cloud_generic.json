      {
          "name": "GenericCloudCloudStagingSystemAccountIngestor",
          "description": "OpenStack cloud account data",
          "class": "DatabaseIngestor",
          "definition_file": "jobs/xdw/system-account.json",
          "endpoints": {
            "source": {
                "type" : "mysql",
                "name" : "Cloud DB",
                "config" : "datawarehouse",
