        "type": "integer"
      },
      "service": {
        "type": "string"
      },
      "state": {
        "type": "string"
      },
      "tenant_id": {
        "type": "string"
      },
      "user_id": {
        "type": "string"
      },
      "user_name": {
        "type": ["string", "null"]
      },
      "vcpus": {
        "type": "integer"
      },
      "volume_id": {
