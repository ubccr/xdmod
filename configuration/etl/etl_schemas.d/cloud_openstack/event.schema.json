      },
      "ephemeral_gb": {
        "type": "string"
      },
      "event_type": {
        "type": "string"
      },
      "generated": {
        "type": "string"
      },
      "host": {
        "type": "string"
      },
      "instance_id": {
        "type": "string"
      },
      "instance_type": {
        "type": "string"
      },
      "instance_type_id": {
        "type": "integer"
      },
      "launched_at": {
        "type": "string",
