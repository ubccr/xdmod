      },
      "memory_mb": {
        "type": "integer"
      },
      "message_id": {
        "type": "string"
      },
      "project_id": {
        "type": "string"
      },
      "project_name": {
        "type": "string"
      },
      "request_id": {
        "type": "string"
      },
      "resource_id": {
        "type": "string"
      },
