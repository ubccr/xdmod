{
    "+realms": {
        "Cloud": {
            "schema": "modw_cloud",
            "table": "cloud_fact",
            "datasource": "Cloud",
            "group_bys": [{
                    "name": "none",
                    "class": "GroupByNone"
                },
                {
                    "name": "person",
                    "class": "GroupByPerson"
                },
                {
                    "name": "account",
                    "class": "GroupByAccount"
                },
                {
                    "name": "configuration",
                    "class": "GroupByConfiguration"
                },
                {
                    "name": "resource",
                    "class": "GroupByResource"
                },
                {
                    "name": "day",
                    "class": "GroupByDay"
                },
                {
                    "name": "month",
                    "class": "GroupByMonth"
                },
                {
                    "name": "quarter",
                    "class": "GroupByQuarter"
                },
                {
                    "name": "year",
                    "class": "GroupByYear"
                },
                {
                    "name": "memory_buckets",
                    "class": "GroupByMemoryUsage"
                },
                {
                    "name": "vm_size",
                    "class": "GroupByVMSize"
                }
            ],
            "statistics": [{
                    "name": "num_vms_ended",
                    "class": "VMCountStatistic"
                },
                {
                    "name": "num_vms_running",
                    "class": "RunningVMCountStatistic",
                    "control": true
                },
                {
                    "name": "num_vms_started",
                    "class": "StartedVMCountStatistic",
                    "control": true
                },
                {
                    "name": "avg_wallduration_hours",
                    "class": "AverageWallHoursStatistic"
                },
                {
                    "name": "avg_core_time",
                    "class": "AverageCoreHoursStatistic"
                },
                {
                    "name": "core_time",
                    "class": "CoreHoursStatistic"
                },
                {
                    "name": "avg_num_cores",
                    "class": "AverageCoresReservedStatistic"
                },
                {
                    "name": "num_cores",
                    "class": "CoresReservedStatistic"
                },
                {
                    "name": "weighted_num_cores",
                    "class": "WeightedCoreWallStatistic"
                },
                {
                    "name": "memory_reserved",
                    "class": "MemoryReservationStatistic"
                },
                {
                    "name": "disk_reserved",
                    "class": "DiskReservationStatistic"
                }
            ]
        }
    }
}
