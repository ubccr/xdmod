{
    "day": {
        "$ref": "datawarehouse.d/ref/group-by-time-period.json#/day"
    },
    "fieldofscience": {
        "$ref": "datawarehouse.d/ref/group-by-hierarchy.json#/fieldofscience"
    },
    "jobsize": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/jobsize"
    },
    "jobwaittime": {
        "attribute_table": "job_wait_times",
        "attribute_table_schema": "modw",
        "attribute_to_aggregate_table_key_map": [
            {
                "id": "job_wait_time_bucket_id"
            }
        ],
        "attribute_values_query": {
            "joins": [
                {
                    "name": "job_wait_times"
                }
            ],
            "orderby": [
                "id"
            ],
            "records": {
                "id": "id",
                "name": "description",
                "order_id": "id",
                "short_name": "description"
            }
        },
        "category": "Administrative",
        "chart_options": {
            "dataset_display_type": {
                "aggregate": "bar",
                "timeseries": "area"
            }
        },
        "data_sort_order": null,
        "description_html": "A categorization of jobs into discrete groups based on the total linear time each job waited.",
        "name": "Job Wait Time"
    },
    "jobwalltime": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/jobwalltime"
    },
    "month": {
        "$ref": "datawarehouse.d/ref/group-by-time-period.json#/month"
    },
    "nodecount": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/nodecount"
    },
    "none": {
        "$ref": "datawarehouse.d/ref/group-by-none.json"
    },
    "nsfdirectorate": {
        "$ref": "datawarehouse.d/ref/group-by-hierarchy.json#/nsfdirectorate"
    },
    "parentscience": {
        "$ref": "datawarehouse.d/ref/group-by-hierarchy.json#/parentscience"
    },
    "person": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/person"
    },
    "pi": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/pi"
    },
    "provider": {
        "$ref": "datawarehouse.d/ref/group-by-common.json#/provider"
    },
    "quarter": {
        "$ref": "datawarehouse.d/ref/group-by-time-period.json#/quarter"
    },
    "queue": {
        "additional_join_constraints": [
            {
                "attribute_expr": "resource_id",
                "operation": "=",
                "aggregate_expr": "task_resource_id"
            }
        ],
        "attribute_table": "queue",
        "attribute_table_schema": "modw",
        "attribute_to_aggregate_table_key_map": [
            {
                "id": "queue"
            }
        ],
        "attribute_values_query": {
            "joins": [
                {
                    "name": "queue"
                }
            ],
            "orderby": [
                "id"
            ],
            "records": {
                "id": "id",
                "name": "id",
                "order_id": "id",
                "short_name": "id"
            }
        },
        "description_html": "Queue pertains to the low level job queues on each resource.",
        "name": "Queue"
    },
    "resource": {
        "attribute_table": "resourcefact",
        "attribute_table_schema": "modw",
        "attribute_to_aggregate_table_key_map": [
            {
                "id": "record_resource_id"
            }
        ],
        "attribute_values_query": {
            "joins": [
                {
                    "alias": "rf",
                    "name": "resourcefact"
                },
                {
                    "alias": "rs",
                    "name": "resourcespecs",
                    "on": "rf.id = rs.resource_id"
                }
            ],
            "orderby": [
                "rf.code",
                "rf.name"
            ],
            "records": {
                "id": "rf.id",
                "name": "rf.code",
                "order_id": "id",
                "short_name": "rf.code"
            },
            "where": [
                "rs.processors IS NOT NULL"
            ]
        },
        "description_html": "A resource is a remote computer that can run jobs.",
        "name": "Resource"
    },
    "resource_type": {
        "attribute_table": "resourcetype",
        "attribute_table_schema": "modw",
        "attribute_to_aggregate_table_key_map": [
            {
                "id": "resourcetype_id"
            }
        ],
        "attribute_values_query": {
            "joins": [
                {
                    "name": "resourcetype"
                }
            ],
            "orderby": [
                "description"
            ],
            "records": {
                "id": "id",
                "name": "description",
                "order_id": "description",
                "short_name": "abbrev"
            }
        },
        "category": "Administrative",
        "description_html": "A categorization of resources into by their general capabilities.",
        "name": "Resource Type"
    },
    "username": {
        "alternate_group_by_columns": [
            "username"
        ],
        "attribute_table": "systemaccount",
        "attribute_table_schema": "modw",
        "attribute_to_aggregate_table_key_map": [
            {
                "id": "systemaccount_id"
            }
        ],
        "attribute_values_query": {
            "joins": [
                {
                    "name": "systemaccount"
                }
            ],
            "orderby": [
                "username"
            ],
            "records": {
                "id": "username",
                "name": "username",
                "order_id": "username",
                "short_name": "username"
            }
        },
        "category": "Administrative",
        "description_html": "The specific system username of the users who ran jobs",
        "name": "System Username"
    },
    "year": {
        "$ref": "datawarehouse.d/ref/group-by-time-period.json#/year"
    }
}
