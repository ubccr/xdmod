            { "$ref": "rawstatistics.d/ref/rawstatistics-common.json#/fields/resource_time_zone" },
            {
                "$ref-with-overwrite": "rawstatistics.d/ref/rawstatistics-common.json#/fields/system_username",
                "$overwrite": {
                    "documentation": "The username on the resource of the gateway proxy user that ran the job. May be a UID or string username depending on the resource."
                }
            },
            {
                "$ref-with-overwrite": "rawstatistics.d/ref/rawstatistics-common.json#/fields/user_id",
                "$overwrite": {
                    "name": "Gateway ID",
                    "documentation": "The numeric identifier assigned by XDMoD to the gateway from which the job was submitted."
                }
            },
            {
                "$ref-with-overwrite": "rawstatistics.d/ref/rawstatistics-common.json#/fields/user_name",
                "$overwrite": {
                    "name": "Gateway",
                    "documentation": "The name of the gateway from which the job was submitted."
                }
            },
            {
                "$ref-with-overwrite": "rawstatistics.d/ref/rawstatistics-common.json#/fields/user_institution_id",
                "$overwrite": {
                    "name": "Organization ID",
                    "documentation": "The numeric identifier assigned by XDMoD to the organization associated with the gateway proxy user that ran the job."
                }
            },
            {
                "$ref-with-overwrite": "rawstatistics.d/ref/rawstatistics-common.json#/fields/user_institution_name",
                "$overwrite": {
                    "name": "Organization",
                    "documentation": "The name of the organization associated with the gateway proxy user that ran the job."
                }
            },
            { "$ref": "rawstatistics.d/ref/rawstatistics-common.json#/fields/pi_id" },
            { "$ref": "rawstatistics.d/ref/rawstatistics-common.json#/fields/pi_name" },
            { "$ref": "rawstatistics.d/ref/rawstatistics-common.json#/fields/pi_institution_id" },
