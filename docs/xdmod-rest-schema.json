                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "^/.*"
                        },
                        "example": "/jupyterhub"
                    }
                ],
                "responses": {
                    "302": {
                        "description": "Redirect with a new JWT in a cookie",
                        "headers": {
                            "Set-Cookie": {
                                "description": "HttpOnly `xdmod_jwt` cookie whose value is a signed JWT containing the user's username",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        { "$ref": "#/components/schemas/warehouse-error-0" },
                                        {
                                            "properties": {
                                                "message": {
                                                    "enum": [
                                                        "next is a required parameter.",
                                                        "Invalid 'next' parameter."
                                                    ]
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
